nt":["<template>\r\n    <div>\r\n        <h3>Detail Result of {{ courseId }}</h3>\r\n        <detail-result :courseId=\"courseId\"></detail-result>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport DetailResult from './DetailResult.vue';\r\nexport default {\r\n  inject: [\"courses\"],  \r\n  components: { DetailResult },\r\n    mounted() {\r\n        const theCourseIdFromURL = this.$route.params.courseId;\r\n        this.courseId = theCourseIdFromURL;\r\n\r\n        if (!this.isAValidCourse(theCourseIdFromURL)) {\r\n            alert(\"The course does not exist\");\r\n            this.$router.push(\"/\");\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            courseId: \"\",\r\n        };\r\n    },\r\n    methods: {\r\n        isAValidCourse(cid) {\r\n            const theCourse = this.courses.filter(rec=>rec.id === cid);\r\n            if (theCourse.length > 0) {\r\n                return true;\r\n            }\r\n            else {\r\n                return false;\r\n            }\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ndiv {\r\n    background: pink;\r\n    padding: 10px;\r\n    margin: 5px;\r\n}\r\n</style>"]}]}